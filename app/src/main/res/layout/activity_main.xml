<?xml version="1.0" encoding="utf-8"?>

<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    >


    <androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">


        <TextView
            android:id="@+id/main"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/baeminjua"
            android:gravity="center"
            android:text="훈족의 민족"
            android:textColor="@color/black"
            android:textSize="20sp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent" />



        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintBottom_toTopOf="@id/et1"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/main"
            tools:ignore="MissingConstraints">


            <ImageView
                android:id="@+id/test1"

                android:layout_width="match_parent"
                android:layout_height="match_parent"

                android:src="@drawable/img" />


        </LinearLayout>



        <EditText
            android:id="@+id/et1"
            android:layout_width="0dp"
            android:layout_height="35dp"
            android:layout_marginTop="300dp"
            android:background="@drawable/edit_custom"
            android:drawableLeft="@drawable/search_1"
            android:gravity="center"
            android:hint="포장/방문 주문하고 싶은 가게, 메뉴 검색"
            android:textColor="@color/black"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@id/main" />





        <ImageView
            android:id="@+id/iv1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="12dp"
            android:layout_marginTop="50dp"

            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@id/et1" />


        <TextView
            android:id="@+id/tv1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/baeminjua"
            android:text="1인분"
            android:textColor="@color/black"
            android:textSize="18sp"
            app:layout_constraintLeft_toLeftOf="@id/iv1"
            app:layout_constraintRight_toRightOf="@id/iv1"
            app:layout_constraintTop_toBottomOf="@id/iv1" />


        <ImageView
            android:id="@+id/iv2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="25dp"
            android:layout_marginTop="50dp"
            android:src="@mipmap/ic_launcher_round"
            app:layout_constraintLeft_toRightOf="@id/iv1"
            app:layout_constraintTop_toBottomOf="@id/et1" />

        <TextView
            android:id="@+id/tv2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/baeminjua"
            android:text="배민오더"
            android:textColor="@color/black"
            android:textSize="18sp"
            app:layout_constraintLeft_toLeftOf="@id/iv2"
            app:layout_constraintRight_toRightOf="@id/iv2"
            app:layout_constraintTop_toBottomOf="@id/iv2" />


        <ImageView
            android:id="@+id/iv3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="25dp"
            android:layout_marginTop="50dp"
            android:src="@mipmap/ic_launcher_round"
            app:layout_constraintLeft_toRightOf="@id/iv2"
            app:layout_constraintTop_toBottomOf="@id/et1" />


        <TextView
            android:id="@+id/tv3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/baeminjua"
            android:text="한식"
            android:textColor="@color/black"
            android:textSize="18sp"
            app:layout_constraintLeft_toLeftOf="@id/iv3"
            app:layout_constraintRight_toRightOf="@id/iv3"
            app:layout_constraintTop_toBottomOf="@id/iv3" />

        <ImageView
            android:id="@+id/iv4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="25dp"
            android:layout_marginTop="50dp"
            android:src="@mipmap/ic_launcher_round"
            app:layout_constraintLeft_toRightOf="@id/iv3"
            app:layout_constraintTop_toBottomOf="@id/et1" />


        <TextView
            android:id="@+id/tv4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/baeminjua"
            android:text="분식"
            android:textColor="@color/black"
            android:textSize="18sp"
            app:layout_constraintLeft_toLeftOf="@id/iv4"
            app:layout_constraintRight_toRightOf="@id/iv4"
            app:layout_constraintTop_toBottomOf="@id/iv4" />

        <ImageView
            android:id="@+id/iv5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="12dp"
            android:layout_marginTop="50dp"
            android:src="@mipmap/ic_launcher_round"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@id/iv1" />


        <TextView
            android:id="@+id/tv5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:fontFamily="@font/baeminjua"
            android:text="카페디저트"
            android:textColor="@color/black"
            android:textSize="18sp"
            app:layout_constraintLeft_toLeftOf="@id/iv5"
            app:layout_constraintRight_toRightOf="@id/iv5"
            app:layout_constraintTop_toBottomOf="@id/iv5" />


        <ImageView
            android:id="@+id/iv6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="25dp"
            android:layout_marginTop="50dp"
            android:src="@mipmap/ic_launcher_round"

            app:layout_constraintLeft_toRightOf="@id/iv5"
            app:layout_constraintTop_toBottomOf="@id/iv2" />

        <TextView
            android:id="@+id/tv6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:fontFamily="@font/baeminjua"
            android:text="돈까스"
            android:textColor="@color/black"
            android:textSize="18sp"
            app:layout_constraintLeft_toLeftOf="@id/iv6"
            app:layout_constraintRight_toRightOf="@id/iv6"
            app:layout_constraintTop_toBottomOf="@id/iv5" />


        <ImageView
            android:id="@+id/iv7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="25dp"
            android:layout_marginTop="50dp"
            android:src="@mipmap/ic_launcher_round"

            app:layout_constraintLeft_toRightOf="@id/iv6"
            app:layout_constraintTop_toBottomOf="@id/iv3" />

        <TextView
            android:id="@+id/tv7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/baeminjua"
            android:text="치킨"
            android:textColor="@color/black"
            android:textSize="18sp"
            app:layout_constraintLeft_toLeftOf="@id/iv7"
            app:layout_constraintRight_toRightOf="@id/iv7"
            app:layout_constraintTop_toBottomOf="@id/iv7" />


        <ImageView
            android:id="@+id/iv8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="25dp"
            android:layout_marginTop="50dp"
            android:src="@mipmap/ic_launcher_round"

            app:layout_constraintLeft_toRightOf="@id/iv7"
            app:layout_constraintTop_toBottomOf="@id/iv4" />

        <TextView
            android:id="@+id/tv8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/baeminjua"
            android:text="피자"
            android:textColor="@color/black"
            android:textSize="18sp"
            app:layout_constraintLeft_toLeftOf="@id/iv8"
            app:layout_constraintRight_toRightOf="@id/iv8"
            app:layout_constraintTop_toBottomOf="@id/iv8" />

        <ImageView
            android:id="@+id/iv9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="12dp"
            android:layout_marginTop="50dp"
            android:src="@mipmap/ic_launcher_round"

            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@id/iv5" />

        <TextView
            android:id="@+id/tv9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/baeminjua"
            android:text="아시안양식"
            android:textColor="@color/black"
            android:textSize="18sp"
            app:layout_constraintLeft_toLeftOf="@id/iv9"
            app:layout_constraintRight_toRightOf="@id/iv9"
            app:layout_constraintTop_toBottomOf="@id/iv9" />

        <ImageView
            android:id="@+id/iv10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="27dp"
            android:layout_marginTop="50dp"
            android:src="@mipmap/ic_launcher_round"

            app:layout_constraintLeft_toRightOf="@id/iv9"
            app:layout_constraintTop_toBottomOf="@id/iv6" />

        <TextView
            android:id="@+id/tv10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/baeminjua"
            android:text="중국집"
            android:textColor="@color/black"
            android:textSize="18sp"
            app:layout_constraintLeft_toLeftOf="@id/iv10"
            app:layout_constraintRight_toRightOf="@id/iv10"
            app:layout_constraintTop_toBottomOf="@id/iv10" />

        <ImageView
            android:id="@+id/iv11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="27dp"
            android:layout_marginTop="50dp"
            android:src="@mipmap/ic_launcher_round"

            app:layout_constraintLeft_toRightOf="@id/iv10"
            app:layout_constraintTop_toBottomOf="@id/iv7" />

        <TextView
            android:id="@+id/tv11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/baeminjua"
            android:text="족발보쌈"
            android:textColor="@color/black"
            android:textSize="18sp"
            app:layout_constraintLeft_toLeftOf="@id/iv11"
            app:layout_constraintRight_toRightOf="@id/iv11"
            app:layout_constraintTop_toBottomOf="@id/iv11" />

        <ImageView
            android:id="@+id/iv12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="27dp"
            android:layout_marginTop="50dp"
            android:src="@mipmap/ic_launcher_round"

            app:layout_constraintLeft_toRightOf="@id/iv11"
            app:layout_constraintTop_toBottomOf="@id/iv8" />

        <TextView
            android:id="@+id/tv12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/baeminjua"
            android:text="야식"
            android:textColor="@color/black"
            android:textSize="18sp"
            app:layout_constraintLeft_toLeftOf="@id/iv12"
            app:layout_constraintRight_toRightOf="@id/iv12"
            app:layout_constraintTop_toBottomOf="@id/iv12" />


    </androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>
